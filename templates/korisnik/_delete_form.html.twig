<form method="post" action="{{ path('korisnik_delete', {'idKorisnik': korisnik.idKorisnik}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ korisnik.idKorisnik) }}">
    <button class="btn">Delete</button>
</form>
