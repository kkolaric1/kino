<form method="post" action="{{ path('dvorana_delete', {'idDvorana': dvorana.idDvorana}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dvorana.idDvorana) }}">
    <button class="btn">Delete</button>
</form>
